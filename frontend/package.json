{
  "name": "material-kit-react",
  "version": "1.9.0",
  "description": "Material Kit React. Coded by Creative Tim",
  "private": true,
  "main": "src/index.js",
  "dependencies": {
    "@material-ui/core": "4.10.0",
    "@material-ui/icons": "4.9.1",
    "@splidejs/splide-extension-grid": "^0.2.0",
