    "react-bootstrap": "^1.5.1",
    "react-datetime": "2.16.3",
    "react-dnd": "^14.0.1",
    "react-dnd-html5-backend": "^14.0.0",
    "react-dom": "16.13.1",
    "react-grid-carousel": "^1.0.1",
    "react-redux": "^7.2.0",
    "react-router": "^6.0.0-beta.0",
    "react-router-dom": "^6.0.0-beta.0",
    "react-scripts": "3.4.1",
    "react-slick": "^0.28.1",
    "react-swipeable-views": "0.13.9",
    "react-toastify": "^7.0.3",
    "redux": "^4.0.5",
