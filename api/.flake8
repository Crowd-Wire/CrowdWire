[flake8]
max-line-length = 127
max-complexity=10
ignore = E501, E203, W503
per-file-ignores =
    __init__.py:F401
    app/db/base.py:F401
    app/api/api_v1/endpoints/websocket.py:C901
exclude =
    alembic
    .git
    __pycache__
    setup.py
    build
    dist
    releases
    .venv
    .pytest_cache
    .vscode
    .github

